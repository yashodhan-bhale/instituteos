{
    "expo": {
        "name": "InstituteOS Staff",
        "slug": "instituteos-staff",
        "version": "0.1.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "scheme": "instituteos",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#1e40af"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.instituteos.staff"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#1e40af"
            },
            "package": "com.instituteos.staff"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router"
        ]
    }
}